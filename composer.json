{
    "name": "arvindkumarbadwal/entrust-mongodb",
    "description": "This package provides a flexible way to add Role-based Permissions to Laravel 5 Moloquent",
    "keywords": ["laravel","illuminate","auth","roles","acl","permission","mongodb","moloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arvindkumar Badwal",
            "email": "arvindkumar.b@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/cache": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "illuminate/database": "~5.0",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-4": {
            "Arvindkumar\\EntrustMongoDB\\": "src/Entrust/"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "Arvindkumar\\EntrustMongoDB\\Entrust\\EntrustServiceProvider"
            ],
            "aliases":{
                "EntrustMongoDB": "Arvindkumar\\EntrustMongoDB\\Entrust\\EntrustFacade"
            }
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Middleware/MiddlewareTest.php"
        ]
    },
    "minimum-stability": "dev"
}
